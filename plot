oc get co -ojson | jq '
[
    .items[] 
    | {"name": .metadata.name, "t": .status.conditions[] 
    | select(.type == "Available").lastTransitionTime}
]' | python3 -c '

def scatter_text(data, x, y, text_column):
    p1 = sns.scatterplot(data, x=x, y=y, size=8, legend=False)

    for line in range(data.shape[0]):
        p1.text(
            data[x][line],
            data[y][line],
            data[text_column][line],
            horizontalalignment="left",
            size="medium",
            color="black",
            weight="semibold",
        )
    return p1

