df = pd.read_json(sys.stdin)
df["t"] = pd.to_datetime(df["t"])
df = df.sort_values(by="t")
from matplotlib import pyplot as plt

df["c"] = range(len(df))
print(df)
import seaborn as sns

s = scatter_text(df, "t", "c", "name")
for rollout in rollouts:
    s.axvline(rollout, color="red")

for rollout in rollouts2:
    s.axvline(rollout, color="green")

for rollout in pulls3:
    s.axvline(rollout, color="blue")

print(rollouts)

plt.show()
